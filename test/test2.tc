-- fibnacci type check
--     普通のfib(5x) -> 5y，つまり，

-- 入力5xの判定．
ndFibIn5amari0 = Nd (
  {q0,q1,q2,q3,q4},
  {q0},
  {q0},
  {q0 -> a(q4,q0),
   q1 -> a(q0,q0),
   q2 -> a(q1,q0),
   q3 -> a(q2,q0),
   q4 -> a(q3,q0)}
)

-- 出力5yの判定．
ndFibOut5amari0 = Nd (
  {q0,q1,q2,q3,q4},
  {q0},
  {q0},
  {q1 -> a(q0,q0),
   q0 -> b(q0,q0), q0 -> b(q1,q4), q0 -> b(q2,q3), q0 -> b(q3,q2), q0 -> b(q4,q1),
   q1 -> b(q0,q1), q1 -> b(q1,q0), q1 -> b(q2,q4), q1 -> b(q3,q3), q1 -> b(q4,q2),
   q2 -> b(q0,q2), q2 -> b(q1,q1), q2 -> b(q2,q0), q2 -> b(q3,q4), q2 -> b(q4,q3),
   q3 -> b(q0,q3), q3 -> b(q1,q2), q3 -> b(q2,q1), q3 -> b(q3,q0), q3 -> b(q4,q4),
   q4 -> b(q0,q4), q4 -> b(q1,q3), q4 -> b(q2,q2), q4 -> b(q3,q1), q4 -> b(q4,q0)}
)

-- 出力5y+4の判定．
ndFibOut5amari4 = Nd (
  {q0,q1,q2,q3,q4},
  {q4},
  {q0},
  {q1 -> a(q0,q0),
   q0 -> b(q0,q0), q0 -> b(q1,q4), q0 -> b(q2,q3), q0 -> b(q3,q2), q0 -> b(q4,q1),
   q1 -> b(q0,q1), q1 -> b(q1,q0), q1 -> b(q2,q4), q1 -> b(q3,q3), q1 -> b(q4,q2),
   q2 -> b(q0,q2), q2 -> b(q1,q1), q2 -> b(q2,q0), q2 -> b(q3,q4), q2 -> b(q4,q3),
   q3 -> b(q0,q3), q3 -> b(q1,q2), q3 -> b(q2,q1), q3 -> b(q3,q0), q3 -> b(q4,q4),
   q4 -> b(q0,q4), q4 -> b(q1,q3), q4 -> b(q2,q2), q4 -> b(q3,q1), q4 -> b(q4,q0)}
)

fibInAlphabet = Alphabets {a}
fibOutAlphabet = Alphabets {a,b}

-- fibnacci.
-- 入力はaの数，出力は#の数をカウント．
-- 普通のFib(0)=0に比べて2進んでいる．
--   fib(0)  = 0,  fib(1)  = 1
--   fib(2)  = 1,  fib(3)  = 2
--   fib(4)  = 3,  fib(5)  = 5
--   fib(6)  = 8,  fib(7)  = 13
--   fib(8)  = 21, fib(9)  = 34
--   fib(10) = 55, fib(11) = 89
tdttFib = Tdtt (
  {fib, aux},
  {fib},
  {fib(a) -> b(fib(1),aux(1)),
   fib(#) -> #,
   aux(a) -> fib(1),
   aux(#) -> a(#,#)}
)

-- fib(5x) -> 5y+4の検査
--typecheck! tdttFib : ndFibIn5amari0(fibInAlphabet) -> ndFibOut5amari4(fibOutAlphabet)
-- fib(5x) -> 5yの検査
typecheck! tdttFib : ndFibIn5amari0(fibInAlphabet) -> ndFibOut5amari0(fibOutAlphabet)
