ndFibIn3amari1 = Nd (
  {q0,q1,q2,qe},
  {q1},
  {qe},
  {q1 -> a(qe,qe),
   q1 -> a(q0,qe),
   q2 -> a(q1,qe),
   q0 -> a(q2,qe)}
)

ndFibOutEven = Nd (
  {q0,q1},
  {q0},
  {q1},
  {q0 -> a(q1,q1),
   q0 -> a(q0,q0),
   q1 -> a(q0,q1),
   q1 -> a(q1,q0)}
)

ndFibOutOdd = Nd (
  {q0,q1},
  {q1},
  {q1},
  {q0 -> a(q1,q1),
   q0 -> a(q0,q0),
   q1 -> a(q0,q1),
   q1 -> a(q1,q0)}
)

fibAlphabet = Alphabets {a}

tdttFib = Tdtt (
  {fib, aux},
  {fib},
  {fib(a) -> a(fib(1),aux(1)),
   fib(#) -> #,
   aux(a) -> fib(1),
   aux(#) -> #}
)

typecheck! fibAlphabet fibAlphabet ndFibIn3amari1 ndFibOutEven tdttFib
typecheck! fibAlphabet fibAlphabet ndFibIn3amari1 ndFibOutOdd tdttFib
